# {{ ansible_managed }}

CTF_DBHOST="{{ ctf_gameserver_db_host }}"
CTF_DBNAME="{{ ctf_gameserver_db_name }}"
CTF_DBUSER="{{ ctf_gameserver_db_user_vpnstatus }}"
CTF_DBPASSWORD="{{ ctf_gameserver_db_pass_vpnstatus }}"

CTF_LOGLEVEL="{{ ctf_gameserver_vpnstatus_loglevel }}"
CTF_METRICS_LISTEN="{{ ctf_gameserver_vpnstatus_metrics_listen }}"

{% if ctf_gameserver_vpnstatus_wireguard_ifpattern != '' %}
CTF_WIREGUARD_IFPATTERN="{{ ctf_gameserver_vpnstatus_wireguard_ifpattern }}"
{% endif %}

{% if ctf_gameserver_vpnstatus_gateway_ippattern != '' %}
CTF_GATEWAY_IPPATTERN="{{ ctf_gameserver_vpnstatus_gateway_ippattern }}"
{% endif %}
{% if ctf_gameserver_vpnstatus_demo_ippattern != '' %}
CTF_DEMO_IPPATTERN="{{ ctf_gameserver_vpnstatus_demo_ippattern }}"
{% endif %}
{% if ctf_gameserver_vpnstatus_demo_serviceport != '' %}
CTF_DEMO_SERVICEPORT="{{ ctf_gameserver_vpnstatus_demo_serviceport }}"
{% endif %}
{% if ctf_gameserver_vpnstatus_vulnbox_ippattern != '' %}
CTF_VULNBOX_IPPATTERN="{{ ctf_gameserver_vpnstatus_vulnbox_ippattern }}"
{% endif %}
{% if ctf_gameserver_vpnstatus_vulnbox_serviceport != '' %}
CTF_VULNBOX_SERVICEPORT="{{ ctf_gameserver_vpnstatus_vulnbox_serviceport }}"
{% endif %}

{% if ctf_gameserver_vpnstatus_net_numbers_filter_file != '' %}
CTF_NET_NUMBERS_FILTER_FILE="{{ ctf_gameserver_vpnstatus_net_numbers_filter_file }}"
{% endif %}
